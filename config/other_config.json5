{
    "registry": [
        { "path": "inbox/.*" , 
          "with": {
            "url": "inbox/",
            "inbox": "./inbox",
            "inboxPublic": 1,
            "inboxWritable": 1
          }
        },
        { "path": "outbox/.*" , 
          "with": {
            "url": "outbox/",
            "inbox": "./outbox",
            "inboxPublic": 1,
            "inboxWritable": 0
          }
        },
        {
          "path": "markdown/.*" ,
          "do": "./handler/page_markdown.js",
          "with" : {}
        },
        {
          "path": "rdf/.*" ,
          "do": "./handler/page_rdf.js",
          "with": {}
        }
    ]
}